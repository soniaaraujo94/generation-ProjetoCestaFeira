<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ListagemProdutosFragment">

    <androidx.cardview.widget.CardView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:padding="20dp">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <SearchView
                android:id="@+id/searchBar"
                android:layout_width="326dp"
                android:layout_height="48"
                android:background="#EFF8FB"
                android:searchIcon="@drawable/icon_busca" />

            <ImageView
                android:id="@+id/imageView2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:src="@drawable/btn_menu"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toEndOf="@+id/searchBar"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.cardview.widget.CardView>

    <ImageView
        android:id="@+id/usu"
        android:layout_width="53dp"
        android:layout_height="46dp"
        android:layout_marginStart="28dp"
        android:src="@drawable/icon_one"
        app:layout_constraintBottom_toBottomOf="@+id/btnCesta"
        app:layout_constraintStart_toEndOf="@+id/btnCesta"
        app:layout_constraintTop_toTopOf="@+id/btnCesta"
        app:layout_constraintVertical_bias="0.622"
        app:srcCompat="@drawable/btn_perfil"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="53dp"
        android:layout_height="46dp"
        android:layout_marginStart="36dp"
        android:src="@drawable/icon_two"
        app:layout_constraintBottom_toBottomOf="@+id/imageView9"
        app:layout_constraintStart_toEndOf="@+id/imageView9"
        app:layout_constraintTop_toTopOf="@+id/imageView9"
        app:layout_constraintVertical_bias="0.622"
        app:srcCompat="@drawable/btn_cupon"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/btnCesta"
        android:layout_width="53dp"
        android:layout_height="46dp"
        android:layout_marginStart="28dp"
        android:src="@drawable/icon_three"
        app:layout_constraintBottom_toBottomOf="@+id/imageView7"
        app:layout_constraintStart_toEndOf="@+id/imageView7"
        app:layout_constraintTop_toTopOf="@+id/imageView7"
        app:layout_constraintVertical_bias="0.622"
        app:srcCompat="@drawable/btn_carrinho"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="56dp"
        android:layout_height="46dp"
        android:layout_marginStart="56dp"
        android:src="@drawable/icon_four"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/recyclerProdutos"
        app:layout_constraintVertical_bias="0.762"
        app:srcCompat="@drawable/btn_home"
        tools:ignore="MissingConstraints" />

    <androidx.recyclerview.widget.RecyclerView
        android:id="@+id/recyclerProdutos"
        android:layout_width="match_parent"
        android:layout_height="502dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.209" />

</androidx.constraintlayout.widget.ConstraintLayout>